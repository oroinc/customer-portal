            getPrimaryEmailsQb:
                fullNameQueryPart: true
            getSecondaryEmailsQb:
                fullNameQueryPart: true
    properties:
        Oro\Bundle\CustomerBundle\Autocomplete\CustomerUserSearchHandler:
            searchEntities:
                idFieldName: true

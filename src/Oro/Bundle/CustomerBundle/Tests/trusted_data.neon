        Oro\Bundle\CustomerBundle\EventListener\Datagrid\CustomerUserRoleDatagridListener:
            onBuildAfter:
                criteria: true
        Oro\Bundle\CustomerBundle\Handler\CustomerAssignHelper:
            hasAssignments:

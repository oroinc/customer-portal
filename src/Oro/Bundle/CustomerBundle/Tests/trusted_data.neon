        Oro\Bundle\CustomerBundle\Form\Type\FrontendOwnerSelectType:
            configureOptions:
                criteria: true
        Oro\Bundle\CustomerBundle\Entity\Repository\CustomerRepository:
            getBatchIterator:
                fieldName: true

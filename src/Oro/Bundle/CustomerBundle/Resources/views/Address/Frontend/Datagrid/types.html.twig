{% set types = record.getValue('types').toArray() %}
{% set defaultTypes = record.getValue('defaults').toArray() %}

{% set labels = [] %}
{% for type in types %}
    {% set label = type in defaultTypes ? 'oro.customer.customer_typed_address_with_default_type.choice.default_text'|trans({'%type_name%': type.label}) : type.label %}
    {% set labels = labels|merge([label]) %}
{% endfor %}

{{- labels|join(', ') -}}
